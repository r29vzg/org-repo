{
  "name": "dggac-bot",
  "version": "1.0.0",
  "author": "DGG AC Dev Team",
  "description": "A discord.js bot for DGGAC discord server",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "exports": [
    "./dist/start-bot.js",
    "./dist/start-manager.js"
  ],
  "scripts": {
    "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "clean": "git clean -xdf --exclude=\"/config/**/*\"",
    "clean:dry": "git clean -xdf --exclude=\"/config/**/*\" --dry-run",
    "build": "tsc --project tsconfig.json",
    "commands:view": "pnpm run build && node --enable-source-maps dist/start-bot.js commands view",
    "commands:register": "pnpm run build && node --enable-source-maps dist/start-bot.js commands register",
    "commands:rename": "pnpm run build && node --enable-source-maps dist/start-bot.js commands rename",
    "commands:delete": "pnpm run build && node --enable-source-maps dist/start-bot.js commands delete",
    "commands:clear": "pnpm run build && node --enable-source-maps dist/start-bot.js commands clear",
    "start": "pnpm run start:bot",
    "start:bot": "pnpm run build && node --enable-source-maps dist/start-bot.js",
    "start:manager": "pnpm run build && node --enable-source-maps dist/start-manager.js",
    "start:pm2": "pnpm run build && pnpm run pm2:start",
    "pm2:start": "pm2 start process.json",
    "pm2:stop": "pm2 stop process.json",
    "pm2:delete": "pm2 delete process.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@discordjs/rest": "^2.5.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "cron-parser": "^5.3.0",
    "discord.js": "^14.21.0",
    "discord.js-rate-limiter": "1.3.2",
    "dotenv": "^17.2.0",
    "express": "5.1.0",
    "filesize": "10.1.6",
    "linguini": "1.3.1",
    "luxon": "3.6.1",
    "node-fetch": "3.3.2",
    "node-schedule": "2.1.1",
    "pino": "9.7.0",
    "pino-pretty": "13.0.0",
    "pm2": "^6.0.8",
    "reflect-metadata": "^0.2.2",
    "remove-markdown": "0.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/express": "5.0.3",
    "@types/luxon": "3.6.2",
    "@types/node": "^24.0.10",
    "@types/node-schedule": "2.1.7",
    "@types/remove-markdown": "0.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4"
  }
}
